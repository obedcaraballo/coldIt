<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>IdUserController</servlet-name>
        <servlet-class>com.controller.IdUserController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistroUsuarioController</servlet-name>
        <servlet-class>com.controller.RegistroUsuarioController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IdProductoController</servlet-name>
        <servlet-class>com.controller.IdProductoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistroProductoController</servlet-name>
        <servlet-class>com.controller.RegistroProductoController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistroFacturaController</servlet-name>
        <servlet-class>com.controller.RegistroFacturaController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>instituto</servlet-name>
        <servlet-class>instituto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pruebasal</servlet-name>
        <servlet-class>pruebasal</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>IdUserController</servlet-name>
        <url-pattern>/IdUserController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistroUsuarioController</servlet-name>
        <url-pattern>/RegistroUsuarioController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IdProductoController</servlet-name>
        <url-pattern>/IdProductoController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistroProductoController</servlet-name>
        <url-pattern>/RegistroProductoController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistroFacturaController</servlet-name>
        <url-pattern>/RegistroFacturaController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>pruebasal</servlet-name>
        <url-pattern>/pruebasal</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
